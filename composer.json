{
  "name": "phico/cache",
  "description": "Lightweight cache server support for Phico",
  "type": "library",
  "license": "BSD-3-Clause",
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Phico\\Cache\\": "src/"
    }
  },
  "authors": [
    {
      "name": "indgy",
      "email": "me@indgy.uk"
    }
  ],
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "pestphp/pest-plugin": true
    }
  },
  "require": {
    "php": "^8.0",
    "predis/predis": "^2.2",
    "phico/config": "dev-main"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.11",
    "pestphp/pest": "^2.34"
  }
}
